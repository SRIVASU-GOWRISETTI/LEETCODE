class Solution {
public:
    int distributeCandies(vector<int>& candyType) {
        int n =candyType.size();
       set<int> s;
        for(int i=0;i<n;i++){
            s.insert(candyType[i]);
        }
        int doc_advise=n/2;
        int a=s.size();
        return min(a,doc_advise);
        }
};
