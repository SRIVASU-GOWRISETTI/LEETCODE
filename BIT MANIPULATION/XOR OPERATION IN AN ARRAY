class Solution {
public:
    int xorOperation(int n, int start) {
        vector<int> ans(n);
        for(int i=0;i<n;i++){
            ans[i]=start + (2*i);
            }
        int ans1=0;
        for(int i=0;i<ans.size();i++){
          ans1=  ans1^ans[i];
        }
        return ans1;
    }
};
