class Solution {
public:
    ListNode* middleNode(ListNode* head) {
        ListNode* temp;
        temp=head;
        int count=0;
        while(temp!=NULL){
            count++;
            temp=temp->next;
        }
        temp=head;
        int n =count/2;
        while(n>0){
            temp=temp->next;
            
            n--;
        }
        return temp;
    }
};
