class Solution {
public:
    ListNode* oddEvenList(ListNode* head) {
        if(head==NULL){
    return NULL;
}
        ListNode* a=head;
        ListNode* b=head->next;
        ListNode* c=head->next;

        while(a->next!=NULL && b->next!=NULL){
            a->next=b->next;
            a=a->next;
            b->next=a->next;
            b=b->next;
            
        }
   a->next=c;
        return head;
        
    }
};
