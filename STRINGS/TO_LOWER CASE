class Solution {
public:
    string toLowerCase(string s) {
        for(int i=0;i<s.size();i++){
            s[i]=tolower(s[i]);
            
        }
        return s;
        
       /* for(int i=0;i<ans.size();i++){
            if(ans[i]>='A'&& ans[i]<='Z'){
            ans[i]=ans[i]+32;
            
        }
        }
            return ans;*/
            
    }
        
};
